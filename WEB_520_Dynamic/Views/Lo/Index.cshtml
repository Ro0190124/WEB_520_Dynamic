@model IEnumerable<LO>
<div class="container">
	<partial name="_Notification" />
	<div class="Tb-Container">
		<table class="table_user responsive-table">
			<thead>
				<tr>
					<th> STT</th>
					<th> Tên sản phẩm</th>
					<th> Quy cách</th>
					<th> Đơn vị</th>
					<th> Số lượng</th>
					<th> Đơn giá</th>
					<th> Lô</th>
					<th> Hạn sử dụng</th>
				
				</tr>
				
			</thead>
			<tbody>

				@{
					int j = 0;
					double soluong = 0;	
				}
				@foreach (var i in Model )
				{
					j +=1;
					<tr>
						<td>@j</td>
						<td>@i.SAN_PHAM.TenSanPham</td>
						<td>@i.SAN_PHAM.QuyCach</td>
						<td>@i.SAN_PHAM.DonVi</td>
						<td>@i.SoLuong</td>
						<td>@i.SAN_PHAM.DonGia</td>
						<td>@i.TenLo</td>
						<td>@i.HanSuDung</td>
						
					</tr>

				}
			
			</tbody>
			
		</table>
		<input asp-action="HienThiSoLuong" />
	</div>
</div>
